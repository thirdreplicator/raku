{
  "name": "raku",
  "version": "2.1.1",
  "description": "A simple get/put/del interface to Riak",
  "main": "dist/Raku.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill --recursive test/",
    "watch": "mocha --compilers js:babel-core/register --require babel-polyfill --recursive test/ --watch",
    "prebuild": "rimraf dist",
    "build": "babel --out-dir dist src",
    "preactivate": "riak-admin bucket-type create counters '{\"props\":{\"datatype\":\"counter\"}}'",
    "activate": "riak-admin bucket-type activate counters"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thirdreplicator/raku/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thirdreplicator/raku.git"
  },
  "keywords": [
    "riak",
    "raku",
    "simple",
    "easy",
    "crud"
  ],
  "homepage": "https://github.com/thirdreplicator/raku#readme",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.18.0",
    "basho-riak-client": "^2.2.2",
    "no-riak": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.10.2",
    "mocha": "^3.1.2",
    "rimraf": "^2.5.4"
  }
}
